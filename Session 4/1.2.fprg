    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="UN" type="String" array="False" size=""/>
            <declare name="Q" type="Integer" array="False" size=""/>
            <declare name="UP" type="Integer" array="False" size=""/>
            <declare name="TP" type="Integer" array="False" size=""/>
            <output expression="&quot;Please input Item name 'A' or 'B'&quot;" newline="True"/>
            <input variable="UN"/>
            <output expression="&quot;Please input quantity&quot;" newline="True"/>
            <input variable="Q"/>
