                <then>
                    <assign variable="UP" expression="10"/>
                    <assign variable="TP" expression="Q*UP"/>
                </then>
                <else>
                    <assign variable="UP" expression="20"/>
                    <assign variable="TP" expression="Q*UP"/>
                </else>
            </if>
            <output expression="&quot;Item entered is &quot; &amp; UN &amp; &quot;, Unit price is &quot; &amp; UP &amp; &quot;, Extended price is &quot; &amp; TP" newline="True"/>
